<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ezen.jang.mapper.MemberMapper">
  <select id="getTime" resultType="string">
    select sysdate from dual
  </select>
  
  <insert id="insertMember">
  	insert into member (id,name,phone,address)
  	values (#{id},#{name},#{phone},#{address})
  </insert>
  
  <delete id="deleteMember">
  	delete from member where id = #{id}
  </delete>
	
	<select id="selectOneMember" resultType="com.ezen.jang.dto.MemberVO">
		select * from member where id =#{id}
	</select>
	
	<select id="selectAllList" resultType="com.ezen.jang.dto.MemberVO">
		select * from member order by id desc
	</select>
	
</mapper>
